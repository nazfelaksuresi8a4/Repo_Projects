#import sys 
#import sqlite3 as sql

import pandas as pd

random_laught = input('gÃ¼l> ')

random_chars = []
random_val_counts = []

for char in random_laught:
    random_chars.append(char)

copied_char_array = random_chars.copy() #orginal char aray#

random_chars.clear()

for chars in set(copied_char_array):
    random_chars.append(chars)

for count_char in set(copied_char_array):
    random_val_counts.append(copied_char_array.count(count_char))

Data_frame = pd.DataFrame(random_val_counts,index=random_chars)

print(Data_frame)   
  
