import requests
from bs4 import BeautifulSoup

main_target = 'https://news.ycombinator.com/'

response = requests.get(url='https://news.ycombinator.com/')

links = []

soup = BeautifulSoup(response.text,'html.parser')

for parser in soup.find_all('a',href=True):
    href = parser.get('href')

    if href.startswith('https://'):
        links.append(href)
        print(href)
    else:
        pass

    if len(links)>20:
        print('İlk 20 url çekildi!')
        break

