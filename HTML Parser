import requests
from bs4 import BeautifulSoup

response = requests.get('https://news.ycombinator.com/')
soup = BeautifulSoup(response.text, 'html.parser')

links = []

# <a> etiketlerinden href deÄŸerlerini topla
for link in soup.find_all('a', href=True):
    href = link['href']
    links.append(href)

    # 20 linke ulaÅŸtÄ±ysak dÃ¶ngÃ¼yÃ¼ durdur
    if len(links) == 20:
        break

# Linkleri yazdÄ±r
print("ğŸŸ  Ä°lk 20 link:")
for i, link in enumerate(links, 1):
    print(f"{i}. {link}")
